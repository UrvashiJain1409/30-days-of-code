CODE:
import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int mul;
        for(int i=1;i<=10;i++)
        {   
            mul= n*i;
            System.out.println(n+" x "+i+" = "+mul);
        }
    }
}
